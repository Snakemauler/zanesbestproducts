   <header>
        <div class="header-content">
            <div class="header-content-inner">


<p id="notice"><%= notice %></p>
<div class="container">
  <div class="row">
    <div class="col-md-4">
      <p>
        <strong>Title:</strong>
        <%= @post.title %>
      </p>
      <p>
        <strong>Post:</strong>
        <%= @post.body %>
      </p>
      <% if signed_in? && current_user.admin? %>
      <%= link_to 'Edit', edit_post_path(@post) %>
      <% end %> |
      <%= link_to 'Back', posts_path %>
    </div>

      <div class="col-md-4"><%= render 'feedbacks/new_feedback' %> </div>
<div class="col-md-4"><%= render 'feedbacks/feedback_listing' %> </div>


  </div>
</div>

</div>
</div>
</header>